{
  "gitbook": "3.2.2",
  "title": "IceCore Hashids",
  "description": "A lightweight generator for short, unique, non-sequential and decodable Hashids from positive unsigned (long) integer numbers.",
  "author": "Arctic Ice Studio",
  "plugins": ["anchorjs", "github", "toggle-chapters", "toggle-headers", "-sharing"],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/arcticicestudio/icecore-hashids"
    },
    "lunr": {
      "maxIndexSize": 200000
    },
    "theme-default": {
      "styles": {
        "website": "./styles/website.css"
      }
    }
  },
  "variables": {
    "api": {
      "version": "0.3.0"
    }
  }
}
